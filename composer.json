{
    "name": "shamir0xe/php-library",
    "description": "A useful php library which provides functions and libraries",
    "homepage": "https://github.com/shamir0xe/php-library",
    "keywords": [
        "shamir0xe",
        "php-library",
        "php",
        "library"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "shamir0xe",
            "email": "shamir0xe@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "shamir0xe\\PhpLibrary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "shamir0xe\\PhpLibrary\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
